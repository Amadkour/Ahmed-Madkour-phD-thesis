  doi       = {10.1063/1.4913021},
  file      = {:C$\backslash$:/Users/js.angarita/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ortega et al. - 2015 - An improvement to the K-means algorithm oriented to big data(3).pdf:pdf},
  issn      = {0094-243X},
  month     = {apr},
  number    = {1},
  pages     = {820002},
  publisher = {AIP Publishing LLC},
  title     = {{An improvement to the K-means algorithm oriented to big data}},
  url       = {http://aip.scitation.org/doi/abs/10.1063/1.4913021},
  volume    = {1648},
  year      = {2015}
}


