  keywords  = {LEER, Dataset, Evaluation},
  pages     = {573--580},
  file      = {IEEE Xplore Abstract Record:C\:\\Users\\luis.enrique.diez\\Zotero\\storage\\FBPBEUFH\\6385773.html:text/html}
